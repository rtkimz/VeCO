<script> function notready() { alert("This is only a placeholder.\nThe ability to view a .cbr file's contents will be added at a later time.") } </script>
<table class="item_center">
<% @comics.each do |comic| %>
  <tr>
    <td><a href="#" onclick=notready()><%= image_tag "book.png", alt: "book", height: "20" %></a></td>
    <td><%= comic_link_to "#{comic.title.name} ##{comic.issue_number}", comic.full_path %></td>
  </tr>
<% end %>
</table>
